{
    "editor.mouseWheelZoom": true,
    "python.formatting.provider": "none",
    "notebook.compactView": false,
    "files.autoSave": "onFocusChange",
    "editor.fontFamily": "'JetBrainsMono Nerd Font','Iosevka Nerd Font','CaskaydiaCove Nerd Font','JetBrainsMono NF','CaskaydiaCove Nerd Font'",
    "editor.fontSize": 16,
    "editor.lineHeight": 1.7,
    "editor.rulers": [80,120],
    "editor.fontLigatures": true,
    "editor.cursorSurroundingLines": 4,
    "jupyter.themeMatplotlibPlots": false,
    "vim.handleKeys": {
        "<C-d>": true,
        "<space>": true,
        "<Esc>": true,
        "<C-p>": false,
        "<C-g>": false,
        "<C-s>": false,
        "<C-r>": true,
        "<C-w>": false,
        "<A-h>": false,
        "<A-l>": false,
        "<A-u>": true,
        "<C-h>": false,
        "<C-j>": true,
        "<C-k>": false,
        "<C-m>": false,
        "<C-n>": false,
        "<C-x>": false,
        "<C-o>": true,
        "<C-c>": false,
        "<C-e>": true,
        "<C-f>": false,
        "<C-y>": false,
        "<C-z>": false
    },
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>", "i", "i"],
            "commands": ["super-figure.editVector"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [ "C-e" ],
            "after": [ "$" ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [ "C-e" ],
            "after": [ "$" ]
        },
        {
            "before": [ "j" ],
            "after": [ "g", "j" ]
        },
        {
            "before": [ "k" ],
            "after": [ "g", "k" ]
        },
        {
            "before": ["<leader>", "p", "f"],
            "commands": ["workbench.action.quickOpen"]
        },
        {
            "before": ["<leader>", "p", "s"],
            "commands": ["workbench.action.findInFiles"]
        },
        {
            "before": ["<leader>", "."],
            "commands": ["workbench.action.quickOpen"]
        },
        {
            "before": ["<leader>", "b", "k"],
            "commands": ["workbench.action.closeActiveEditor"]
        },
        {
            "before": ["<leader>", "<leader>"],
            "commands": ["workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"]
        },
        {
            "before": ["<leader>", "f", "s"],
            "commands": ["workbench.action.files.save"]
        },
    ],
    "vim.cursorStylePerMode.insert": "block",
    "workbench.startupEditor": "none",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "files.exclude": {
        "**.aux": true,
        "**.fdb_latexmk": true,
        "**.fls": true,
        "**.lof": true,
        "**.idx": true,
        "**.ilg": true,
        "**.thm": true,
        "**.toc": true,
        "**.ist": true,
        "**.bst": true,
        "**.log": true,
        "**.ind": true,
        "**/**.aux": true,
        "**.xdv": true,
        "**.out": true,
        "**.synctex.gz": true,
        "**.bbl": true,
        "**.blg": true,
        "**.acn": true,
        "**.glo": true,
        "**.mst": true,
        "**.xml": true
    },
    "latex-workshop.view.pdf.color.dark.backgroundColor": "//#282a34",
    
    "vim.camelCaseMotion.enable": true,
    "vim.enableNeovim": false,
    "vim.highlightedyank.enable": true,
    "vim.neovimUseConfigFile": false,
    "editor.minimap.autohide": true,
    "editor.minimap.scale": 3,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.maxColumn": 40,
    "editor.minimap.size": "fit",
    "explorer.compactFolders": false,
    "explorer.fileNesting.enabled": true,
    "editor.minimap.enabled": false,
    "latex-workshop.view.pdf.color.dark.pageBorderColor": "grey",
    "extensions.autoUpdate": false,
    "update.enableWindowsBackgroundUpdates": false,
    "git.openRepositoryInParentFolders": "always",
    "vim.useSystemClipboard": true,
    "breadcrumbs.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.tabs.enableAnimation": false,
    "editor.cursorBlinking": "solid",
    "workbench.reduceMotion": "on",
    "window.commandCenter": false,
    "caps-lock-state.status_bar_text": "ðŸ”’ Caps Lock ON!A",
    "latex-workshop.bibtex-format.tab": "4 spaces",
    "notebook.output.fontFamily": "Iosevka Nerd Font",
    "window.autoDetectColorScheme": true,
    "notebook.markup.fontSize": 14,
    "window.density.editorTabHeight": "compact",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.wrapTabs": true,
    "terminal.integrated.tabStopWidth": 4,
    "glassit-linux.opacity": 100,
    "zenMode.hideLineNumbers": false,
    "zenMode.centerLayout": false,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 100,
    "terminal.integrated.fontSize": 16,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "super-figure.vectorFigureEditor": "/usr/bin/inkscape",
    "editor.tabCompletion": "on",
    "vim.leader": "space",
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.sneak": true,
    "super-figure.bitmapFigureEditor": "/usr/bin/gimp",
    "window.menuBarVisibility": "toggle",
    "editor.scrollbar.vertical": "visible",
    "workbench.layoutControl.enabled": false,
    "MATLAB.telemetry": false,
    "MATLAB.installPath": "/usr/local/MATLAB/R2018b/",
    "matlab.mlintpath": "/usr/local/MATLAB/R2018b/bin/glnxa64/mlint",
    "matlab.matlabpath": "/usr/local/MATLAB/R2018b/bin/matlab",
    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "vscode-icons",
    "terminal.integrated.commandsToSkipShell": [
        "matlab.interrupt"
    ],
    "MATLAB.matlabConnectionTiming": "onDemand",
    "editor.defaultFormatter": "AffenWiesel.matlab-formatter",
    "workbench.activityBar.location": "hidden",
}
