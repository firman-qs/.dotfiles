{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Quantum tunneling capital italized": {
		"prefix": "Quant",
		"body": [
			"\\textit{Quantum Tunneling}"
		]
		"description": "Menuliskan quantum tunneling italic"
	},
	"Single input cite": {
		"prefix": "\\cite",
		"body": [
			"\\cite{$1}"
		]
		"description": "Menuliskan sitasi"
	},
	"Quantum tunneling small italized": {
		"prefix": "quant",
		"body": [
			"\\textit{quantum tunneling}"
		]
		"description": "Menuliskan quantum tunneling italic"
	},
	"Figure Environtment with ... width": {
		"prefix": "\\beginfig",
		"body": [
			"\\begin{figure}",
				"\t\\centering",
				"\t\\includegraphics[width=$1\\textwidth]{$2}",
				"\t\\caption{$3}",
				"\t\\labfig{$4}",
			"\\end{figure}"
		]
		"description": "Menambahkan environtment figure pada latex"
	}
}